<wux-select id="sel-person" />
<wux-cascader visible="{{ visible1 }}" default-value="{{ value1 }}" title="所在地区" options="{{ options1 }}" bind:close="onClose1" bind:change="onChange1" />
<view class="page">
    <view class="head">
        <wux-cell-group>
            <wux-cell hover-class="none">
                {{adrsInfo.departTitle}}
            </wux-cell>
            <wux-cell hover-class="none">
                {{adrsInfo.arriveTitle}}
            </wux-cell>
            <!--<wux-cell hover-class="none">-->
                <!--<wux-cell title="人数" extra="{{ title1 }}" bind:click="onClick1"></wux-cell>-->
            <!--</wux-cell>-->
            <!--<wux-cell hover-class="none">-->
                <!--<wux-cell title="日期" extra="{{ title1 }}" bind:click="onClick1"></wux-cell>-->
            <!--</wux-cell>-->
        </wux-cell-group>

        <wux-row>
            <wux-col span="6">
                <wux-cell title="人数" extra="{{ personInfo.title }}" bind:click="onPersonClick"></wux-cell>
            </wux-col>
            <wux-col span="6">
                <wux-cell title="时间" extra="{{ title1 }}" bind:click="onDateOpen"></wux-cell>
            </wux-col>
        </wux-row>
    </view>

    <view class="page-body">
        <map class='map' id='myMap'  scale="12" longitude="{{longitude}}" latitude="{{latitude}}"
             show-location="true" markers="{{markers}}"
             bindregionchange='regionChange'   style="width: {{mapWidth}}px; height: {{mapHeight}}px;">
            <cover-view class="service-type">
                <cover-view>

                </cover-view>
            </cover-view>
        </map>

    </view>
</view>
